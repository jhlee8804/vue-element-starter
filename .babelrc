{
  "presets": [
    ["@babel/preset-env", {
      "modules": "commonjs",
      "useBuiltIns": false,
      "targets": {
        "browsers": [
          "> 1%",
          "last 2 versions",
          "ie >= 11"
        ],
        "node": "current"
      }
    }]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-syntax-dynamic-import"
  ]
}
